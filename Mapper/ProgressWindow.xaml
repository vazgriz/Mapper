<Window x:Class="Mapper.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mapper"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        Title="Generating" Height="90" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*" MaxHeight="25"/>
        </Grid.RowDefinitions>
        <Label Name="ProgressLabel" Grid.Row="0" Content="Generating..."/>
        <ProgressBar Name="ProgressBar" Grid.Row="1" Margin="5,0,5,5" Minimum="0" Maximum="100" Value="0" />
    </Grid>
</Window>
